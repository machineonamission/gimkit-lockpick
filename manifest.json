{
  "manifest_version": 3,
  "name": "GimKit Lock-pick",
  "description": "Crack GimKit wide open",
  "version": "0.0.1",
  "action": {
    "default_popup": "interface/index.html"
  },
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "64": "logo64.png",
    "128": "logo128.png",
    "1000": "logo.png"
  },
  "permissions": ["declarativeContent"],
  "host_permissions": [
    "*://*.gimkit.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "injector.js"
      ],
      "matches": [
        "*://*.gimkit.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "foreground.js"
      ],
      "matches": [
        "*://*.gimkit.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "bg.js"
  }
}